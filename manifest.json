{
	"name": "元気が出るスイッチ",
	"manifest_version": 3,
	"version": "0.1",
	"permissions": ["activeTab","scripting","tabs"],
    "content_scripts": [
      {
        "matches":  ["http://*/*", "https://*/*"],
        "js": ["content_script.js"]
      }
    ],
	"background": {
		"service_worker": "alert.js"
	},
	"action": {}
}